{
    "name": "{{#lambda.lowercase}}{{invokerPackage}}{{/lambda.lowercase}}/api",
    "description": "Generated API package from OpenAPI specification",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/validation": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "{{invokerPackage}}\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{invokerPackage}}\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "{{invokerPackage}}\\Providers\\ApiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
