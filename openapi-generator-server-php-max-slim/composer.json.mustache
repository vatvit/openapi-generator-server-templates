{
    "name": "{{#lambda.lowercase}}{{invokerPackage}}{{/lambda.lowercase}}/api",
    "description": "Generated API package from OpenAPI specification",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.0",
        "php-di/php-di": "^7.0",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "{{invokerPackage}}\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{invokerPackage}}\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
