{
    "name": "{{#lambda.lowercase}}{{invokerPackage}}{{/lambda.lowercase}}/api",
    "description": "Generated API package from OpenAPI specification",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/http-foundation": "^6.4|^7.0",
        "symfony/validator": "^6.4|^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "{{invokerPackage}}\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{invokerPackage}}\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
